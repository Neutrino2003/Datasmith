# Deepgram API Key (for audio transcription)
DEEPGRAM_API_KEY=your_deepgram_api_key_here

# Google Gemini API Key
GOOGLE_API_KEY=your_google_api_key_here

# LLM Model (gemini-2.0-flash recommended)
